<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Jeysmook_CustomerPrices::base" title="Jeysmook Customer Prices" sortOrder="100">
                    <resource id="Jeysmook_CustomerPrices::config" title="Module Configuration" sortOrder="10"/>
